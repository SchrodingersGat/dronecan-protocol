<?xml version="1.0"?>

<Protocol name="DroneCAN"
          prefix="DC_"
          api="1"
          version="1.0"
          supportSpecialFloat="false"
          supportInt64="false"
          supportFloat64="false"
          endian="big"
          pointer="DCPkt"
          >

<!-- Include top-level enumerations -->
<Require file="dc_message_classes.xml"/>
<Require file="dc_vendors.xml"/>


<!-- Include various group message definition files -->

<!-- Command message class -->
<Require file="msg_classes/command.xml"/>

<!-- Telemetry message class -->
<Require file="msg_classes/telemetry.xml"/>

<!-- System Info Message Class -->
<Require file="msg_classes/system.xml"/>

<!-- Block data transfer message class -->
<Require file="msg_classes/block.xml"/>

</Protocol>
