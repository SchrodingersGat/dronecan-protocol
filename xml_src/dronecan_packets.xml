<?xml version="1.0"?>

<Protocol name="DroneCAN_Packets" file="dronecan_packets">

<Enum name="DroneCAN_System_Packets" file="dronecan_packets" lookup="true" lookupTitle="true" prefix="PKT_DC_">
    
    <!-- System information packets -->
    <Value name="SYS_STATUS" value="0xF000" comment="Device status information"/>

    <Value name="SYS_UID" value="0xF100" comment="Unique Identifer for DroneCAN node"/>

    <Value name="SYS_MANF_STRING" value="0xF105" comment="Manufacturer device identifier string"/>
    <Value name="SYS_USER_STRING" comment="User device identifier string"/>

    <Value name="SYS_FW_VERSION" value="0xF110" comment="Firmware version information"/>
    <Value name="SYS_FW_DATE" comment="Firmware date information"/>
    <Value name="SYS_FW_SETTINGS" comment="User-configurable settings information"/>

    <Value name="SYS_HW_INFO" value="0xF115" comment="Hardware information"/>


</Enum>

</Protocol>